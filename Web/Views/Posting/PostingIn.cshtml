<style>
    .form-group .labelprofile {
        font-weight:600;
    }
</style>
<div class="container mt-2">
    <div class="card mb-3 ">
        <div class="card-header">

            <span id="SpnTxnId" class="d-none">0</span>
            @*<span id="PtoId" class="d-none">0</span>*@
            <div class="row">
                <div class="col-xl-12 text-left">
                    <h3>
                       Posting Out Indl Army No
                        <small class="text-muted"><span id="iarmynopostingin"></span></small>
                        
                    </h3>
                </div>
            </div>
        </div>
        <div class="card-body" style="min-height:150px;">
            <div class="row g-3 row-deck mb-3">

                @* <div class="card p-4">
                <div class="mt-2">

                </div>
                </div> *@
               
                <form action="" id="SaveForm" method="post" name="SaveForm">


                    <span class="spnBasicDetailIdOutID d-none">0</span>
                    <span class="spnPostingOutID d-none">0</span>

                    <span class="spnFromAspNetUsersId d-none">0</span>
                    <span class="spnFromUnitID d-none">0</span>
                    <span class="spnFromUserID d-none">0</span>
                    <span class="spnToUserID d-none">0</span>

                    <span class="spnRequestId d-none">0</span>

                    <div class="row">
                        <div class="col-md-12 ">
                            <div class="card cardbg minheight mb-2 mt-2">
                                <div class="seven">
                                    <h1>Personal Details</h1>
                                </div>
                                <div class="mt-2 ml-4">

                                    <div class="row">
                                        
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Regd User Dtls</label>
                                                <div class="col-sm-8">
                                                    <label id="lblRegdUser" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Name</label>
                                                <div class="col-sm-8">
                                                    <label id="lblName" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Category</label>
                                                <div class="col-sm-8">
                                                    <label id="lblCategory" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Unit/SUS No</label>
                                                <div class="col-sm-8">
                                                    <label id="lblUnitname" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Appt</label>
                                                <div class="col-sm-8">
                                                    <label id="lblAppt" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Status of Inds I Card</label>
                                                <div class="col-sm-8">
                                                    <label id="lblStatusofInds" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Tracking Id</label>
                                                <div class="col-sm-8">
                                                    <label id="lblTracking" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Photos</label>
                                                <div class="col-sm-8">
                                                   <img src="" id="pstimage" height="50" width="50" />

                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 ">
                            <div class="card cardbg minheight mb-2 mt-2">
                                <div class="mt-2">
                                    <div class="seven">
                                        <h1>Transfer Out</h1>
                                    </div>
                                    <div class="mt-2 ml-4 mr-4">
                                    <div class="row">
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Posting Type</label>
                                                    <div class="col-sm-8">
                                                        <label class="form-label">Posting Out</label>

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Reason</label>
                                                    <div class="col-sm-8">
                                                        <select class="form-select" id="ddlpostingReason" required>
                                                            <option value="">Select One</option>
                                                          

                                                        </select>

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Authority</label>
                                                    <div class="col-sm-8">
                                                        <input type="text" id="txtAuthority" class="form-control" placeholder="Enter Authority" maxlength="50" required />

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">SOS Date</label>
                                                    <div class="col-sm-8">
                                                        <input type="date" id="txtSosDate" class="form-control" placeholder="Enter SOS Date" required />

                                                    </div>

                                                </div>
                                            </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-4 labelprofile">Unit Name</label>
                                                <div class="col-sm-8">
                                                        <input type="text" class="form-control" id="postingoutUnitName" placeholder="Enter Unit Name" maxlength="50" required />
                                                        <span class="d-none" id="postingoutUnitId"></span>
                                                </div>

                                            </div>
                                        </div>
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Army No</label>
                                                    <div class="col-sm-8">
                                                        <select class="form-select" id="ddlaspnetiserpostout" required>
                                                            <option value="">Select One</option>
                                                            
                                                        </select>

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Name</label>
                                                    <div class="col-sm-8">
                                                        <label class="form-label" id="lblToName"></label>

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Domain Id</label>
                                                    <div class="col-sm-8">
                                                        <label class="form-label" id="lblToDomainId"></label>

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Appt</label>
                                                    <div class="col-sm-8">
                                                        <label class="form-label" id="lbltoAppt"></label>

                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-md-12 text-center mb-4">
                                                <input type="button" class="btn btn-primary" id="btnPostingOut" value="Posting Out" />
                                            </div>
                                      </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    
                    </div>
               </form>
            </div>
        </div>
    </div>
</div>
<script src="~/developerjs/pastingin.js"></script>
<script src="~/developerjs/mtables.js"></script>