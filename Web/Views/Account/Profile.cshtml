@inject IHttpContextAccessor HttpContextAccessor
@model DTOProfileAndMappingRequest
@{
    Layout = null;

    DTOTempSession? dTOTempSession = new DTOTempSession();
    StringBuilder m1 = new StringBuilder();
    StringBuilder m2 = new StringBuilder();
    if (!string.IsNullOrEmpty(@HttpContextAccessor.HttpContext.Session.GetString("IMData")))
    {
        dTOTempSession = SessionHeplers.GetObject<DTOTempSession>(HttpContextAccessor.HttpContext.Session, "IMData");
        m1.Append("Status : " + dTOTempSession.Status);
        m1.Append(",<br/>AdminFlag : " + dTOTempSession.AdminFlag);
        m1.Append(",<br/>DomainId : " + dTOTempSession.DomainId);
        m1.Append(",<br/>RoleName : " + dTOTempSession.RoleName);
        m1.Append(",<br/>ICNO : " + dTOTempSession.ICNO);
        m1.Append(",<br/>UserId : " + dTOTempSession.UserId);
        m1.Append(",<br/>TDMUnitMapId : " + dTOTempSession.TDMUnitMapId);
        m1.Append(",<br/>TDMId : " + dTOTempSession.TDMId);
        m1.Append(",<br/>AspNetUsersId : " + dTOTempSession.AspNetUsersId);

        m2.Append("ICNoDomainId : " + dTOTempSession.ICNoDomainId);
        m2.Append(",<br/>ICNoTDMUnitMapId : " + dTOTempSession.ICNoTDMUnitMapId);
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/x-icon" href="/images/AFSAC.png">
    <title> E-IASAC </title>
    @*  <link href="/Fonts/css/all.min.css" rel="stylesheet" type="text/css"> *@

    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/nunito.css" rel="stylesheet" />
    <link href="~/css/roboto.css" rel="stylesheet" />
    <link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/jqueryui/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/css/login.css" rel="stylesheet" />
    <link href="~/fonts/css/all.min.css" rel="stylesheet" />

    <link href="~/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <link href="~/css/feed.css" rel="stylesheet" />

    <link href="~/css/normalize.min.css" rel="stylesheet" />
    @*   <link href="~/css/swiper.min.css" rel="stylesheet" /> *@
    <link href="~/css/feed.css" rel="stylesheet" />
    <link href="~/css/main.css" rel="stylesheet" />
    @*     <link href="~/css/sliders/style.css" rel="stylesheet" /> *@
    <style>
        .swal2-confirm, .swal2-styled {
            background: #D97924 !important;
        }
    </style>
    <style>
        .row.mb-1.required .form-label::after {
            content: "*";
            color: red;
        }

        .login-wrap {
            margin-top: 50px;
            margin-bottom: 100px;
            /* border: 7px double #242c6d; */
            border-radius: 10px;
            box-shadow: 0px 0px 40px rgb(217 121 4 / 23%);
            z-index: 1;
            margin-left: 20%;
            margin-right:30%
        }

        body {
            /*   background-image: url('/images/logo-white.jpg'); */
            background-size: cover;
            background-color: #0d2470;
            background-repeat: no-repeat;
        }
        .unit-label-background {
            background-color: #e9ecef;
            max-width: 200px;
            min-width: 200px;
        }
    </style>
</head>
<body>
    <div class="page">
        <div class="container-fluid">
            <div class="row bg-white login-wrap align-items-center">
                <div class="mb-1 logo-sec text-center">
                    <img src="/Images/AFSAC_Logo.png" style="max-width:40%" />
                </div>
                <p class="font-weight-normal text-center mb-4 mt-1 ">Profile</p>
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-xl-12">
                        <form asp-antiforgery="true" asp-controller="Account" asp-action="Profile" id="Profile" name="Profile" method="post" class="mt-3">
                            <div class="card-body">
                                <div class="row">
                                    <input hidden asp-for="UserId" />
                                    <input hidden asp-for="UnitMapId" />
                                    <input hidden asp-for="ApptId" />
                                    <div class="col-md-6 col-lg-6 col-xl-6">
                                        <div class="row mb-2 required">
                                            <label asp-for="ArmyNo" class="col-sm-4 col-form-label form-label">Offrs Army No<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                <input asp-for="ArmyNo" class="form-control" placeholder="" readonly />
                                                <span asp-validation-for="ArmyNo" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="RankId" class="col-sm-4 col-form-label form-label">Rank<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                @if (Model.UserId > 0)
                                                {
                                                    <select asp-for="RankId" class="form-select" asp-items="ViewBag.OptionsRank" disabled="disabled">
                                                    </select>
                                                }
                                                else
                                                {
                                                    <select asp-for="RankId" class="form-select" asp-items="ViewBag.OptionsRank">
                                                    </select>
                                                }

                                                <span asp-validation-for="RankId" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="Name" class="col-sm-4 col-form-label form-label">Name<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                @if (Model.UserId > 0)
                                                {
                                                    <input asp-for="Name" class="form-control" placeholder="" readonly />
                                                }
                                                else
                                                {
                                                    <input asp-for="Name" class="form-control" placeholder="" />
                                                }
                                                <span asp-validation-for="Name" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="MobileNo" class="col-sm-4 col-form-label form-label">Mobile No<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                @if (Model.UserId > 0)
                                                {
                                                    <input asp-for="MobileNo" class="form-control allow-number" placeholder="" readonly />
                                                }
                                                else
                                                {
                                                    <input asp-for="MobileNo" class="form-control allow-number" placeholder="" />
                                                }
                                                <span asp-validation-for="MobileNo" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="DialingCode" class="col-sm-4 col-form-label form-label">ASCON Dialing<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                @if (Model.UserId > 0)
                                                {
                                                    <input asp-for="DialingCode" class="form-control allow-number" placeholder="" readonly />
                                                }
                                                else
                                                {
                                                    <input asp-for="DialingCode" class="form-control allow-number" placeholder="" />
                                                }
                                                <span asp-validation-for="DialingCode" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="Extension" class="col-sm-4 col-form-label form-label">Extension<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                @if (Model.UserId > 0)
                                                {
                                                    <input asp-for="Extension" class="form-control allow-number" placeholder="" readonly />
                                                }
                                                else
                                                {
                                                    <input asp-for="Extension" class="form-control allow-number" placeholder="" />
                                                }
                                                <span asp-validation-for="Extension" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="IsIO" class="col-sm-4 col-form-label form-label">Initating Offr<i class="fas fa-asterisk mand1"></i></label>
                                            @if (Model.UserId > 0)
                                            {
                                                <div class="col-sm-8">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsIO" value="true" id="IsIOyes" checked=@(Model.IsIO == true?true:false) disabled="disabled">
                                                        <label class="form-check-label" for="IsIOyes">
                                                            Yes
                                                        </label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsIO" value="false" id="IsIOno" checked=@(Model.IsIO == false?true:false) disabled="disabled">
                                                        <label class="form-check-label" for="IsIOno">
                                                            No
                                                        </label>
                                                    </div>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="col-sm-8">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsIO" value="true" id="IsIOyes">
                                                        <label class="form-check-label" for="IsIOyes">
                                                            Yes
                                                        </label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsIO" value="false" id="IsIOno">
                                                        <label class="form-check-label" for="IsIOno">
                                                            No
                                                        </label>
                                                    </div>
                                                    <span class="text-danger field-validation-valid" id="IsIO-error"></span>
                                                </div>
                                            }

                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="IsCO" class="col-sm-4 col-form-label form-label">Commanding Offr<i class="fas fa-asterisk mand1"></i></label>
                                            @if (Model.UserId > 0)
                                            {
                                                <div class="col-sm-8">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsCO" value="true" id="IsCOyes" checked=@(Model.IsCO == true?true:false) disabled="disabled">
                                                        <label class="form-check-label" for="IsCOyes">
                                                            Yes
                                                        </label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsCO" value="false" id="IsCOno" checked=@(Model.IntOffr == false?true:false) disabled="disabled">
                                                        <label class="form-check-label" for="IsCOno">
                                                            No
                                                        </label>
                                                    </div>
                                                </div>

                                            }
                                            else
                                            {
                                                <div class="col-sm-8">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsCO" value="true" id="IsCOyes">
                                                        <label class="form-check-label" for="IsCOyes">
                                                            Yes
                                                        </label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IsCO" value="false" id="IsCOno">
                                                        <label class="form-check-label" for="IsCOno">
                                                            No
                                                        </label>
                                                    </div>
                                                    <span class="text-danger field-validation-valid" id="IsCO-error"></span>
                                                </div>
                                            }

                                        </div>
                                        <div class="row mb-2 required">
                                            <label asp-for="IntOffr" class="col-sm-4 col-form-label form-label"> Int Offrs.<i class="fas fa-asterisk mand1"></i>    </label>
                                            @if (Model.UserId > 0)
                                            {
                                                <div class="col-sm-8">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IntOffr" value="true" id="intoffsyes" checked=@(Model.IntOffr == true?true:false) disabled="disabled">
                                                        <label class="form-check-label" for="intoffsyes">
                                                            Yes
                                                        </label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IntOffr" value="false" id="intoffsno" checked=@(Model.IntOffr == false?true:false) disabled="disabled">
                                                        <label class="form-check-label" for="intoffsno">
                                                            No
                                                        </label>
                                                    </div>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="col-sm-8">
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IntOffr" value="true" id="intoffsyes">
                                                        <label class="form-check-label" for="intoffsyes">
                                                            Yes
                                                        </label>
                                                    </div>
                                                    <div class="form-check form-check-inline">
                                                        <input class="form-check-input" type="radio" name="IntOffr" value="false" id="intoffsno">
                                                        <label class="form-check-label" for="intoffsno">
                                                            No
                                                        </label>
                                                    </div>
                                                    <span class="text-danger field-validation-valid" id="IntOffr-error"></span>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                    <div class="col-md-6 col-lg-6 col-xl-6">
                                        <div class="row mb-2 required">
                                            <span id="spnUnitAppointmentId" class="d-none">@(Model.ApptId > 0 ? Model.ApptId : 0)</span>
                                            <label class="col-form-label col-sm-4">Appointment Name<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                <input class="form-control" placeholder="Type Appointment" id="txtAppointmentName" name="txtAppointmentName" data-val="true" data-val-regex="This special chars not allowed for security reasons." data-val-required="Appointment Name is required." data-val-regex-pattern="^[\w \& \()]*$" />
                                                <span class="text-danger field-validation-valid" data-valmsg-for="txtAppointmentName" data-valmsg-replace="true"></span>
                                            </div>
                                        </div>
                                        <div class="form-group row mb-2">
                                            <span id="spnUnitMapId" class="d-none">@(Model.UnitMapId > 0 ? Model.UnitMapId : 0)</span>
                                            <label class="col-form-label col-sm-4">Unit Name<i class="fas fa-asterisk mand1"></i></label>
                                            <div class="col-sm-8">
                                                <div class="d-flex">
                                                    <input class="form-control" placeholder="Type Unit Name" id="txtUnitName" name="txtUnitName" data-val="true" data-val-regex="This special chars not allowed for security reasons." data-val-required="Unit Name is required." data-val-regex-pattern="^[\w ]*$" /><button type="button" id="btnAddUnit"><i class='fas fa-plus'></i></button>
                                                </div>
                                                <span class="text-danger field-validation-valid" data-valmsg-for="txtUnitName" data-valmsg-replace="true"></span>
                                            </div>
                                        </div>
                                        <div class="row mb-2">
                                            <label class="col-form-label col-sm-4">Sus No</label>
                                            <div class="col-sm-8">
                                                <label id="lblSusno" class="col-form-label unit-label-background"></label>
                                            </div>
                                        </div>
                                        <div id="lbl1" class="row mb-2 d-none">
                                            <label class="col-form-label col-sm-4 ">PSO / Dte</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label unit-label-background" id="lblPso"></label>
                                            </div>
                                        </div>
                                        <div id="lbl2" class="row mb-2 d-none">
                                            <label class="col-form-label col-sm-4 ">DG / Sub Dte</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label unit-label-background" id="lblDG"></label>
                                            </div>
                                        </div>
                                        <div id="lbl3" class="row mb-2 d-none">
                                            <label class="col-form-label col-sm-4 ">Comd / PSO</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label unit-label-background" id="lblComd"></label>
                                            </div>
                                        </div>
                                        <div id="lbl4" class="row mb-2 d-none">
                                            <label class="col-form-label col-sm-4">Corps / Dte / Area</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label unit-label-background" id="lblCorps"></label>
                                            </div>
                                        </div>
                                        <div id="lbl5" class="row mb-2 d-none">
                                            <label class="col-form-label col-sm-4">Div / Branch / Sub Area</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label unit-label-background" id="lblDiv"></label>
                                            </div>
                                        </div>
                                        <div id="lbl6" class="row mb-2 d-none">
                                            <label class="col-form-label col-sm-4">Bde</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label unit-label-background" id="lblBde"></label>
                                            </div>
                                        </div>
                                        <div id="lbl7" class="row mb-2 d-none">
                                            <label class="col-form-label col-sm-4">Type of Fmn / Branch</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label unit-label-background" id="lblFmn"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-footer text-center">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <input type="button" class="btn btn-primary" value="Submit" id="btnSubmit" onclick="Proceed()" />
                                @if (TempData["success"] != null)
                                {
                                    <div class="alert alert-success alert-dismissible fade show" role="alert">
                                        <span class="text-success">@TempData["success"]</span>
                                        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                                    </div>
                                }
                                @if (TempData["error"] != null)
                                {
                                    <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                        <span class="text-danger">@TempData["error"]</span>
                                        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                                    </div>
                                }
                            </div>
                            @{

                            }
                            @if (dTOTempSession != null)
                            {
                                <div class="alert alert-info alert-dismissible fade show" role="alert">
                                    <span class="text-info text-break">@Html.Raw(@m1)</span>
                                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                                </div>
                                <div class="alert alert-info alert-dismissible fade show" role="alert">
                                    <span class="text-info text-break">@Html.Raw(@m2)</span>
                                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                                </div>
                            }
                        </form>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <script asp-src-include="~/lib/jquery/jquery.js"></script>
    <script asp-src-include="~/lib/jquery-validate/jquery.validate.js" defer></script>
    <script asp-src-include="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js" defer></script>
    <script asp-src-include="~/lib/jqueryui/jquery-ui.min.js"></script>
    <script asp-src-include="~/bootstrap/js/bootstrap.min.js"></script>
    <script asp-src-include="~/developerjs/mtables.js"></script>
    <script asp-src-include="~/sweetalert2/sweetalert2.min.js"></script>
    <script asp-src-include="~/developerjs/accountprofile.js"></script>
    <script asp-src-include="~/developerjs/keyconstants.js"></script>
</body>
</html>
<div class="modal fade" id="AddNewUnitmap" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Enter Unit Details With Mapping</h5>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <form action="" id="SaveUnitWithMap" method="post" name="SaveUnitWithMap">
                        <div class="row">
                            @if (dTOTempSession!=null)
                            {
                                <span class=" d-none" id="spnServiceNo">@dTOTempSession.ICNOInput</span>
                                <span class=" d-none" id="spnName">NA</span>
                                <span class=" d-none" id="spnRank">NA</span>
                                <span class=" d-none" id="spnDomainId">@dTOTempSession.DomainId</span>
                            }
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">SUS No <i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <input class="form-control" placeholder="Enter SUS No" name="txtSusno" id="txtSusno" minlength="8" maxlength="8" data-val="true" data-val-regex="Use Seven digit numbers and one alphabet in capital letter without space" data-val-required="SUS No is required." data-val-regex-pattern="^[0-9]{7}[A-Z]{1}$" />
                                    <span class="text-danger field-validation-valid" data-valmsg-for="txtSusno" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Unit <i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <input class="form-control" placeholder="Enter Unit" name="txtUnit" id="txtUnit" maxlength="100" data-val="true" data-val-regex="This special chars not allowed for security reasons." data-val-required="Unit is required." data-val-regex-pattern="^[\w ]+$" />
                                    <span class="text-danger field-validation-valid" data-valmsg-for="txtUnit" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Select Unit Type <i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="UnitTyperdi" id="UnitType1" value="1" checked>
                                        <label class="form-check-label" for="UnitType1">Unit</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="UnitTyperdi" id="UnitType2" value="2">
                                        <label class="form-check-label" for="UnitType2">Unit is Fmn HQ</label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="UnitTyperdi" id="UnitType3" value="3">
                                        <label class="form-check-label" for="UnitType3">Unit is Dte/Branch</label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group row mb-2 unittype">
                                <label class="col-form-label col-sm-4">Comd / PSO<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <select class="form-select" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlCommand" id="ddlCommand" data-val="true" data-val-regex="This special chars not allowed for security reasons." data-val-required="Comd / PSO is required." data-val-regex-pattern="^[\d]+$">
                                        <option value="0">Select Comd / PSO </option>
                                    </select>
                                    <span class="text-danger field-validation-valid" data-valmsg-for="ddlCommand" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-4 unittype">
