@{
    ViewData["Title"] = "User Regn";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .model-body-background {
        /* background-color: #e9ecef; */
    }

    .model-body-label{
       /*  background-color: #6189c3; */
    }
</style>

<div class="container mt-4">
    <div class="card mb-3 ">
        <div class="card-header">

            <span id="SpnTxnId" class="d-none">0</span>
            @*<span id="PtoId" class="d-none">0</span>*@
            <div class="row">
                <div class="col-xl-12 text-left">
                    <h3>
                        User Regn Details
                        <small class="text-muted"></small>
                    </h3>
                </div>
            </div>
        </div>
        <div class="card-body" style="min-height:150px;">
            <div class="row">
                <div class="col-xl-6">
                </div>
                <div class="col-xl-6">
                    <div class="d-flex d-md-flex justify-content-md-between align-items-md-center">
                        <h6 class="mb-3 mb-md-0">
                            @* Total Command <span id="lblTotal" class="ml-3 border border-light b px-2 py-1">0</span> *@
                        </h6>
                        <div class="d-flex">
                            @* <button type="button" id="btnAdd" class="btn btn-outline-success font-weight-600 btn-sm">Add Command</button> *@
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="choice" id="Id" value="Id">
                                <label for="choice">Id</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="choice" id="DomainId" value="DomainId">
                                <label for="choice">Domain Id</label>
                            </div>
                            <div class="form-check form-check-inline">
                                <input class="form-check-input" type="radio" name="choice" id="ICNo" value="ICNo">
                                <label for="choice">IC No</label>
                            </div>
                            <div>
                                <input class="btn-outline-light form-control mr-2" placeholder="Search Id / Domain /IC No" id="txtSearch" maxlength="30" pattern="^[a-zA-Z0-9 ]*$" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="table-responsive">
                        <table class="table border border-purple table-striped no-footer table-hover" role="grid" id="tbldata">
                            <thead>
                                <tr>
                                    <th class="noExport d-none"></th>
                                    <th class="nowrap">S No</th>
                                    <th class="nowrap">Reg Id</th>
                                    <th class="nowrap">Domain Id</th>
                                    <th class="nowrap">IC No</th>
                                    <th class="nowrap">Role</th>
                                    <th class="nowrap">Requested Genr On (DT)</th>
                                    <th class="nowrap">Mapping</th>
                                    <th class="nowrap">IsActive</th>
                                    <th class="nowrap">Status</th>
                                    <th class="nowrap">Action</th>
                                </tr>
                            </thead>
                            <tbody id="DetailBody">
                            </tbody>
                        </table>
                    </div>
            </div>
        </div>
    </div>
</div>
<script asp-src-include="~/developerjs/mtables.js"></script>
<script asp-src-include="~/developerjs/userregn.js"></script>
<script asp-src-include="~/developerjs/commandvalidation.js"></script>
<div class="modal fade" id="AddDomainFlag" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Enter Domain Details With Profile</h5>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <form action="" id="UpdateDomainFlag" name="UpdateDomainFlag" method="post">
                        <div class="row">
                            <span class=" d-none" id="spnDomainRegId">0</span>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Domain Id<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <input class="form-control" placeholder="Enter Domain Id" id="txtDomainId" name="txtDomainId" maxlength="20" data-val="true" data-val-regex="This special chars and space not allowed for security reasons." data-val-required="Domain Id is required." data-val-regex-pattern="^[\w\-]+$" />
                                    <span class="text-danger field-validation-valid" data-valmsg-for="txtDomainId" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Role<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <input class="form-control" placeholder="Enter Role" id="txtRole" name="txtRole" data-val="true" data-val-regex="This special chars not allowed for security reasons." data-val-required="Role is required." data-val-regex-pattern="^[\w ]*$" />
                                    <span class="text-danger field-validation-valid" data-valmsg-for="txtRole" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-4">
                                <label class="col-form-label col-sm-4">Approval<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtapproval" value="true" id="txtapprovalyes" data-val-regex-pattern="^[\w]*$">
                                        <label class="form-check-label" for="txtapprovalyes">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtapproval" value="false" id="txtapprovalno" data-val-regex-pattern="^[\w]*$">
                                        <label class="form-check-label" for="txtapprovalno">
                                            No
                                        </label>
                                    </div>
                                    <span class="text-danger field-validation-valid" id="txtapproval-error"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-4">
                                <label class="col-form-label col-sm-4">Active<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtactive" value="true" id="txtactiveyes" data-val="true" data-val-regex-pattern="^[\w]*$">
                                        <label class="form-check-label" for="txtactiveyes">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtactive" value="false" id="txtactiveno" data-val="true" data-val-regex-pattern="^[\w]*$">
                                        <label class="form-check-label" for="txtactiveno">
                                            No
                                        </label>
                                    </div>
                                    <span class="text-danger field-validation-valid" id="txtactive-error"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <span id="spnTrnDomainMappingId" class="d-none">0</span>
                                <span id="spnUnitAppointmentId" class="d-none">0</span>
                                <label class="col-form-label col-sm-4">Appointment Name<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <input class="form-control Alphanumeric" placeholder="Type Appointment" id="txtAppointmentName" name="txtAppointmentName" data-val="true" data-val-regex="This special chars not allowed for security reasons." data-val-required="Appointment Name is required." data-val-regex-pattern="^[\w \& \()]*$" />
                                    <span class="text-danger field-validation-valid" data-valmsg-for="txtAppointmentName" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <span id="spnUnitMapId" class="d-none">0</span>
                                <label class="col-form-label col-sm-4">Unit Name<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <input class="form-control Alphanumeric" placeholder="Type Unit Name" id="txtUnitName" name="txtUnitName" data-val="true" data-val-regex="This special chars not allowed for security reasons." data-val-required="Unit Name is required." data-val-regex-pattern="^[\w ]*$" />
                                    <span class="text-danger field-validation-valid" data-valmsg-for="txtUnitName" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Sus No</label>
                                <div class="col-sm-8">
                                    <label id="lblSusno" class="col-form-label unit-label-background"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Comd</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblComd"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Corps</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblCorps"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Div</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblDiv"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Bde</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblBde"></label>
                                </div>
                            </div>
                            <div class="col-sm-12 text-right">
                                <input type="button" class="btn btn-primary" value="Save" id="btnDomainAdd" onclick="Proceed()" />
                                <button type="button" id="btnDomainAddReset" class="btn btn-outline-danger">Reset</button>
                            </div>
                            <div class="col-sm-12" id="msg">
                            </div>
                        </div>
                    </form>

                    <div class="col-sm-12" id="msg">
                    </div>
                </div>
            </div>

        </div><!-- modal-content -->
    </div><!-- modal-dialog -->
</div>

<div class="modal fade" id="AddMapping" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Mapping Add / Remove</h5>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body model-body-background">
                <div class="row">
                    <form action="" id="SaveMapping" name="SaveMapping" method="post">
                        <div class="row">
                            <span class=" d-none" id="spnDomainRegIdForMapping">0</span>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4 model-body-label">Domain Id</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblDomainIdForMapping"></label>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Role</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblRoleForMapping"></label>
                                </div>
                            </div>
                            <div class="form-group row mb-4">
                                <label class="col-form-label col-sm-4">Approval</label>
                                <div class="col-sm-8">
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtapproval" value="true" id="txtapprovalyesForMapping" disabled>
                                        <label class="form-check-label" for="txtapprovalyesForMapping">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtapproval" value="false" id="txtapprovalnoForMapping" disabled>
                                        <label class="form-check-label" for="txtapprovalnoForMapping">
                                            No
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group row mb-4">
                                <label class="col-form-label col-sm-4">Active</label>
                                <div class="col-sm-8">
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtactive" value="true" id="txtactiveyesForMapping" disabled>
                                        <label class="form-check-label" for="txtactiveyesForMapping">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check form-check-inline">
                                        <input class="form-check-input" type="radio" name="txtactive" value="false" id="txtactivenoForMapping" disabled>
                                        <label class="form-check-label" for="txtactivenoForMapping">
                                            No
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <span class=" d-none" id="spnTrnDomainMappingIdForMapping">0</span>
                                <label class="col-form-label col-sm-4">Appointment Name</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblAppointmentNameForMapping"></label>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Unit Name</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblUnitNameForMapping"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Sus No</label>
                                <div class="col-sm-8">
                                    <label id="lblSusnoForMapping" class="col-form-label unit-label-background"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Comd</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblComdForMapping"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Corps</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblCorpsForMapping"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Div</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblDivForMapping"></label>
                                </div>
                            </div>
                            <div class="row mb-2">
                                <label class="col-form-label col-sm-4">Bde</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblBdeForMapping"></label>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <span class=" d-none" id="spnUserProfileId">0</span>
                                <label class="col-form-label col-sm-4">Offrs Army No</label>
                                <div class="col-sm-8">
                                    <input class="form-control Alphanumeric" placeholder="Enter Army No" id="txtArmyNo" name="txtArmyNo" minlength="7" maxlength="15" data-val="true" data-val-regex="This special chars and space not allowed for security reasons." data-val-regex-pattern="^[\w\-]+$" />
                                    <span class="text-danger field-validation-valid" data-valmsg-for="txtArmyNo" data-valmsg-replace="true"></span>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Name</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblName"></label>
                                </div>
                            </div>
                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Rank</label>
                                <div class="col-sm-8">
                                    <label class="col-form-label unit-label-background" id="lblRank"></label>
                                </div>
                            </div>
                            <div class="col-sm-12 text-right">
                                <input type="button" class="btn btn-primary" value="Save" id="btnAddMapping" onclick="ProceedForMapping()" />
                            </div>
                            <div class="col-sm-12" id="msg">
                            </div>
                        </div>
                    </form>

                    <div class="col-sm-12" id="msg">
                    </div>
                </div>
            </div>

        </div><!-- modal-content -->
    </div><!-- modal-dialog -->
</div>