<div class="modal fade" id="AddNewUnitmap" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Enter Unit Details With Mapping</h5>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>

            </div>

            <div class="modal-body">

                <div class="row">



                    <form action="" id="SaveFormMapUnit" method="post" name="SaveForm">
                        <div class="row">
                            <span class=" d-none" id="spnUnitMapUnitId">0</span>
                            <div class="form-group row mb-2">

                                <label class="col-form-label col-sm-4">SUS No <i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">


                                    <input class="form-control Alphanumeric" placeholder="Enter SUS No" id="txtSusno" minlength="8" maxlength="8" required pattern="^[a-zA-Z0-9 ]*$" title="Only Alphabets and Numbers allowed" />
                                </div>

                            </div>

                            <div class="form-group row mb-2">

                                <label class="col-form-label col-sm-4">Unit <i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">

                                    <span id="SpnUnitMapId" class="d-none">0</span>
                                    <input class="form-control Alphanumeric" placeholder="Enter Unit" id="txtUnit" required pattern="^[a-zA-Z0-9 ]*$" title="Only Alphabets and Numbers allowed" />

                                </div>

                            </div>
                            <div class="form-group row mb-2">

                                <label class="col-form-label col-sm-4">Fmn HQ/Branch <i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" id="ChkFmnBrach">
                                        <label class="form-check-label" for="ChkFmnBrach">
                                            Fmn HQ/Branch
                                        </label>
                                    </div>

                                </div>

                            </div>
                            <div class="form-group row mb-2">

                                <label class="col-form-label col-sm-4">Comd / PSO<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">

                                    <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlCommand" required id="ddlCommand">
                                        <option value="0">Select Comd / PSO </option>

                                    </select>
                                </div>

                            </div>


                            <div class="form-group row mb-4">

                                <label class="col-form-label col-sm-4">Corps / Dte / Area<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">

                                    <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlCorps" required id="ddlCorps">
                                        <option value="0">Select Corps / Dte / Area </option>

                                    </select>
                                </div>

                            </div>

                            <div class="form-group row mb-2">

                                <label class="col-form-label col-sm-4">Div  /Branch / SubArea<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">

                                    <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlDiv" required id="ddlDiv">
                                        <option value="0">Select Div  /Branch / SubArea </option>

                                    </select>
                                </div>

                            </div>

                            <div class="form-group row mb-2">
                                <label class="col-form-label col-sm-4">Bde<i class="fas fa-asterisk mand1"></i></label>
                                <div class="col-sm-8">

