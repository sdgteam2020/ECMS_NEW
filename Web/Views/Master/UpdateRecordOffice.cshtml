@{
    ViewData["Title"] = "Add Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .form-group .labelprofile {
        font-size: 14px;
        font-weight: 700;
    }

    .cardbg {
        /*  background: rgb(255,255,255);
                background: radial-gradient(circle, rgba(255,255,255,0) 0%, rgba(63,251,217,0.3225665266106442) 100%); */
    }

    .minheight {
        min-height: 225px;
    }
</style>
<header class="page-header page-header-dark bg-gradient-primary-to-secondary pb-10">
    <div class="containerMain">
        <div class="page-header-content pt-4">
            <div class="row align-items-center justify-content-between">
                <div class="col-auto">
                    <h1 class="page-header-title">
                        <div class="page-header-icon"> <i class="fa fa-bars"></i></div>
                        <a href="/Home/Index" class="mr-2">Home</a> > <a href="/Home/Dashboard" class="mr-2">Dashboard</a> > <a href="/Home/DashboardUserMgt" class="mr-2"> User Mngt</a> >Profile Details

                    </h1>
                </div>
            </div>

        </div>
    </div>
</header>
<div class="containerMain mt-n10">
    <div class="row">
        <div class="col-12 mb-4">
            <div class="card h-100">
                <div class="card-header">
                    <div class="row">
                        <div class="col-xl-12 text-left">
                            <h3>
                                Update Mapping in Record Office
                                <small class="text-muted"> </small>
                            </h3>
                        </div>
                    </div>
                </div>
                <div class="card-body" style="min-height:150px;">
                    <div class="row">
                        <form action="" id="UpdateRecordOffice" method="post" name="UpdateRecordOffice">
                            <div class="row">
                                <span class="d-none" id="spnRecordOfficeId">0</span>
                                <span class="d-none" id="spnUnitMapId">@ViewBag.UnitId</span>
                                <div class="row">
                                    <div class="card minheight cardbg mb-2 mt-2">
                                        <div class="mt-2 ml-4 ">
                                            <div class="row">
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Record Office</label>
                                                    <div class="col-sm-8">
                                                        <label id="lblROName" class="col-form-label"></label>
                                                    </div>
                                                </div>
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4  labelprofile">Abbreviation</label>
                                                    <div class="col-sm-8">
                                                        <label id="lblROAbbreviation" class="col-form-label"></label>
                                                    </div>
                                                </div>
                                                <div class="form-group row mb-2">
                                                    <label class="col-form-label col-sm-4 labelprofile">Name of Arms, Service & Corps</label>
                                                    <div class="col-sm-8">
                                                        <label class="col-form-label " id="lblArms"></label>
                                                    </div>
                                                </div>
                                                <div class="form-group row mb-2">
                                                    <span id="spnTrnDomainMappingId" class="d-none">@ViewBag.TDMId</span>
                                                    <label class="col-form-label col-sm-4 fw-bold">Select Domain Id <i class="fas fa-asterisk mand1"></i></label>
                                                    <div class="col-sm-8">
                                                        <select class="form-select" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlDomainId" required id="ddlDomainId">
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-12 text-right mt-2">
                                    <input type="button" class="btn btn-primary mt-4 mr-4" value="Submit" id="btnProfilesave" />
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script asp-src-include="~/developerjs/UpdateRecordOffice.js"></script>
