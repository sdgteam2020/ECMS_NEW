@model List<DTOICardTypeRequest>
@{
    ViewBag.Title = "Select";
}
<style>
    a{ text-decoration:none;}
</style>
<div class="row g-3">
    <div class="card-header py-3 d-flex  bg-transparent">
        <h6 class="mb-0 fw-bold">@ViewBag.Title</h6>
    </div>
    <div class="card-body">
        <div class="row g-3 mb-3">
            @foreach (var item in Model)
            {
                <div class="col-xl-3 col-lg-3 col-md-3 col-sm-6">
                    <div class="card">
                        <a asp-controller="BasicDetail" asp-action="Registration" asp-route-Id="@item.EncryptedId">
                            <div class="card-body">
                                @item.Name
                            </div>
                        </a>
                    </div>
                </div>
            }
        </div>
    </div>
</div>