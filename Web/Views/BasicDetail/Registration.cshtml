@model DTORegistrationRequest
@inject IHttpContextAccessor HttpContextAccessor
@{
    ViewBag.Title = "New Registration";
}
<style>
    .form-group.row.mb-2.required .form-label::after {
        content: "*";
        color: red;
    }
</style>
<div class="container mt-2">
    <div class="card mb-3 ">
        <div class="card-header">

            <span id="SpnTxnId" class="d-none">0</span>
            @*<span id="PtoId" class="d-none">0</span>*@
            <div class="row">
                <div class="col-xl-12 text-left">
                    <h3>
                        Request Details For Army No
                        <small class="text-muted"><span id="icarddetails"></span></small>
                        @* <small class="text-muted" id="icarddetails"></small> *@
                    </h3>
                </div>
            </div>
        </div>
        <div class="card-body" style="min-height:150px;">
            <div class="row g-3 row-deck mb-3">

                @* <div class="card p-4">
                <div class="mt-2">

                </div>
                </div> *@
                <div class="card ">

                    <div class="row">
                        <div class="col-md-12 ">
                            <div class="card cardbg minheight mb-2 mt-2">
                                <div class="seven">
                                    <h1>Request Details</h1>
                                </div>
                                <div class="mt-2 ml-4">

                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-6 labelprofile">Regd User Detls</label>
                                                <div class="col-sm-6">
                                                    <label id="RegdUser" class="col-form-label "></label>

                                                </div>

                                            </div>
                                        </div>

                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-6 labelprofile">Category</label>
                                                <div class="col-sm-6">
                                                    <label id="lblCategory" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-6 labelprofile">Unit Name</label>
                                                <div class="col-sm-6">
                                                    <label id="lblunitname" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="form-group row mb-2">
                                                <label class="col-form-label col-sm-6 labelprofile">Reason For Apply</label>
                                                <div class="col-sm-6">
                                                    <label id="lblReason" class="col-form-label"></label>

                                                </div>

                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12 ">
                            <div class="card cardbg minheight mb-2 mt-2">
                                <div class="mt-2">
                                    <div class="seven">
                                        <h1>Get Data From API</h1>
                                    </div>
                                    <div class="row">
                                        <form enctype="multipart/form-data" asp-controller="BasicDetail" asp-action="Registration" id="Registration" name="Registration" method="post" class="mt-3">
                                            <div class="card-body">
                                                @*  <input hidden asp-for="DOB" />

                                                <input hidden asp-for="DateOfCommissioning" />

                                                <input hidden asp-for="TypeId"/>*@
                                               @*  <input hidden asp-for="State" />
                                                <input hidden asp-for="District" />
                                                <input hidden asp-for="PS" />
                                                <input hidden asp-for="PO" />
                                                <input hidden asp-for="Tehsil" />
                                                <input hidden asp-for="Village" />
                                                <input hidden asp-for="PinCode" /> *@
                                                <input hidden asp-for="IdenMark1" />
                                                <input hidden asp-for="IdenMark2" />
                                                <input hidden asp-for="AadhaarNo" />
                                                @* <input hidden asp-for="Height" /> *@
                                                <input hidden asp-for="BloodGroup" />
                                                <input hidden asp-for="ApplyForId" />
                                                <input hidden asp-for="RegistrationId" />
                                                <input hidden asp-for="TypeId" />
                                                <input hidden asp-for="RemarksIds" />
                                                <form id="Reg" name="Reg" method="post">
                                                    <div class="row">
                                                        @* <div class="col-xl-4">
                                                        <div class="form-group row mb-2 required">
                                                        <label asp-for="RegistrationId" class="col-form-label col-sm-6 form-label"></label>
                                                        <div class="col-sm-6">
                                                        <select asp-for="RegistrationId" class="form-select" asp-items="ViewBag.OptionsRegistration">
                                                        </select>
                                                        <span asp-validation-for="RegistrationId" class="text-danger"></span>
                                                        </div>
                                                        </div>
                                                        </div> *@
                                                        <div class="col-xl-6">
                                                            <div class="form-group row mb-2 required">
                                                                <label class="col-form-label col-sm-6 form-label">Service No</label>
                                                                <div class="col-sm-3">
                                                                    <input asp-for="ServiceNumber" readonly class="form-control" />
                                                                    <span asp-validation-for="ServiceNumber" class="text-danger"></span>
                                                                </div>
                                                                <div class="col-sm-3">
                                                                    <button type="button" id="btngetdata" name="btngetdata" class="btn btn-primary" onclick="getData(this.form.id);">Fetch Data</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        @*  <div class="col-xl-2"> *@
                                                        @*  <button type="button" class="btn btn-round btn-primary" id="btngetToken">Get ICNO From Token</i></button> *@

                                                        @*   </div> *@
                                                        <div class="col-xl-6">
                                                            <div class="form-group row mb-2 required">
                                                                <label asp-for="RankId" class="col-sm-6 col-form-label form-label"></label>
                                                                <div class="col-sm-6">
                                                                    <select asp-for="RankId" class="form-select" required>
                                                                    </select>
                                                                    <span asp-validation-for="RankId" class="text-danger"></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </form>
                                                <div class="row">
                                                    <div class="col-xl-6">

                                                        <div class="form-group row mb-2">
                                                            <label asp-for="Name" class="col-form-label col-sm-6"></label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="Name" class="form-control text-uppercase" placeholder="Max 36 Characters incl spaces" />
                                                                <span asp-validation-for="Name" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2">
                                                            <label class="col-form-label col-sm-6">Dt of Birth (DD-MM-YYYY)</label>
                                                            <div class="col-sm-6">
                                                                @* <input type="text" id="DOB_" name="DOB_" class="form-control" readonly /> *@
                                                                <input asp-for="DOB" class="form-control" readonly />
                                                                <span asp-validation-for="DOB" class="text-danger"></span>
                                                                @* <span id="lblDOB" class="text-danger"></span> *@
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 persAddress">
                                                            <label class="col-form-label col-sm-6">
                                                                Permt Address as per Service Records
                                                            </label>
                                                            <div class="col-sm-6">
                                                                <textarea id="PermanentAddress" rows="6" class="form-control text-uppercase" placeholder="" readonly></textarea>

                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 entryaddress">
                                                            <label class="col-form-label col-sm-6 ">State</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="State" class="form-control paddress text-uppercase" />
                                                                <span asp-validation-for="State" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 entryaddress">
                                                            <label class="col-form-label col-sm-6">PS</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="PS" class="form-control text-uppercase" />
                                                                <span asp-validation-for="PS" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 entryaddress">
                                                            <label class="col-form-label col-sm-6">Village</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="Village" class="form-control text-uppercase" />
                                                                <span asp-validation-for="Village" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 entryaddress">
                                                            <label class="col-form-label col-sm-6 text-uppercase">Pin Code</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="PinCode" class="form-control" />
                                                                <span asp-validation-for="PinCode" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-xl-6">
                                                        <div class="form-group row mb-2">
                                                            <label class="col-form-label col-sm-6 ">Service No</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="ServiceNo" class="form-control text-uppercase" placeholder="IC-12345X/JC-12345X" readonly />
                                                                <span asp-validation-for="ServiceNo" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2">
                                                            <label class="col-form-label col-sm-6">Dt of Commissioning/ Enrolment</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="DateOfCommissioning" class="form-control" readonly />
                                                                <span asp-validation-for="DateOfCommissioning" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 entryaddress">
                                                            <label class="col-form-label col-sm-6">District</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="District" class="form-control text-uppercase" />
                                                                <span asp-validation-for="District" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 entryaddress">
                                                            <label class="col-form-label col-sm-6">PO</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="PO" class="form-control text-uppercase" />
                                                                <span asp-validation-for="PO" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 entryaddress">
                                                            <label class="col-form-label col-sm-6">Tehsil</label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="Tehsil" class="form-control text-uppercase" />
                                                                <span asp-validation-for="Tehsil" class="text-danger"></span>
                                                            </div>
                                                        </div>
                                                        <div class="form-group row mb-2 required">
                                                            <label asp-for="SubmitType" class="col-form-label col-sm-6 form-label">
                                                                I hereby certify that all the details are correct and I am ready to proceed for filling I Card Appl.

                                                            </label>
                                                            <div class="col-sm-6">
                                                                <input asp-for="SubmitType" type="radio" value="1" onclick="registrationEnableDisabledField(this.value)" />
                                                                <label>Yes</label>
                                                                <input asp-for="SubmitType" type="radio" value="2" onclick="registrationEnableDisabledField(this.value)" />
                                                                <label>No</label>
                                                                <span asp-validation-for="SubmitType" class="text-danger"></span>
                                                            </div>
                                                        </div>

                                                        @*  <div class="form-group row mb-2">
                                                        <label  class="col-form-label col-sm-6">
                                                        Raise Observation (s)
                                                        </label>
                                                        <div class="col-sm-6">
                                                        <textarea asp-for="Observations" rows="2" class="form-control" placeholder="" readonly></textarea>
                                                        <span id="lblObservations" name="lblObservations" class="text-danger"></span>
                                                        </div>
                                                        </div> *@
                                                    </div>
                                                </div>
                                                <div class="col-xl-12 Remarks d-none">
                                                    <div class="form-group row mb-2">
                                                        <label class="col-form-label col-sm-2">Remarks</label>
                                                        <div class="col-sm-10">

                                                            <select class="form-control select2 select2-accessible" multiple="" placeholder="Please select a Photos" style="width: 100%;" tabindex="-1" aria-hidden="true" name="Remarks" required id="ddlRemarks">
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-xl-12">
                                                    <div class="d-flax text-center">
                                                        <button type="reset" id="btnclear" name="btnclear" class="btn btn-primary">Clear</button>
                                                        <button type="button" id="btnsubmit" name="btnsubmit" class="btn btn-primary" disabled onclick="return Proceed(this.form.id)">Proceed</button>
                                                    </div>
                                                    <div class="col-xl-12 mt-2">
                                                        <div class="col-sm-12 text-center">
                                                            <div id="tokenmsg" class="d-grid gap-2 col-4 mx-auto">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

