@model DTORegistrationRequest
@inject IHttpContextAccessor HttpContextAccessor
@{
    ViewBag.Title = "Registration";
}
<div class="card-body">
    <div class="row">
        <div class="col-xl-12">
            <div class="row mb-3 required">
                <label asp-for="RegistrationType" class="col-sm-3 col-form-label form-label"></label>
                <div class="col-sm-4">
                    <select asp-for="RegistrationType" class="form-control" asp-items="ViewBag.OptionsRegistrationType">
                    </select>
                    <span asp-validation-for="RegistrationType" class="text-danger"></span>
                </div>
            </div>
            <div class="row mb-3 required">
                <label asp-for="ServiceNumber" class="col-sm-3 col-form-label form-label"></label>
                <div class="col-sm-4">
                    <input asp-for="ServiceNumber" class="form-control" placeholder="IC-12345X/JC-12345X" />
                    <span asp-validation-for="ServiceNumber" class="text-danger"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="card-footer text-center">
    <button type="button" id="btngetdata" name="btngetdata" class="btn btn-primary" onclick="getUserData();">Fetch Data</button>
</div>
<div class="row g-3 row-deck mb-3">
    <div class="card mb-4">
        <div class="card-header py-3 d-flex  bg-transparent">
            <h6 class="mb-0 fw-bold"></h6>
        </div>
        <form enctype="multipart/form-data" asp-controller="BasicDetail" asp-action="Registration" id="Registration" name="Registration" method="post" class="mt-3">
            <div class="card-body">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="row mb-3 required">
                            <label asp-for="Name" class="col-sm-3 col-form-label form-label"></label>
                            <div class="col-sm-4">
                                <input asp-for="Name" class="form-control" placeholder="Max 36 Characters incl spaces" readonly />
                                <span asp-validation-for="Name" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row mb-3 required">
                            <label asp-for="ServiceNo" class="col-sm-3 col-form-label form-label"></label>
                            <div class="col-sm-4">
                                <input asp-for="ServiceNo" class="form-control" placeholder="IC-12345X/JC-12345X" />
                                <span asp-validation-for="ServiceNo" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <label asp-for="DOB" class="col-sm-3 col-form-label form-label"></label>
                            <div class="col-sm-4">
                                <input asp-for="DOB" type="date" class="form-control" max="2999-12-31" />
                                <span asp-validation-for="DOB" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row mb-3">
                            <label asp-for="DateOfCommissioning" class="col-sm-3 col-form-label form-label"></label>
                            <div class="col-sm-4">
                                <input asp-for="DateOfCommissioning" type="date" class="form-control" max="2999-12-31" />
                                <span asp-validation-for="DateOfCommissioning" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="row mb-3 required">
                            <label asp-for="PermanentAddress" class="col-sm-3 col-form-label form-label"></label>
                            <div class="col-sm-8">
                                <input asp-for="PermanentAddress" class="form-control" placeholder="" />
                                <span asp-validation-for="PermanentAddress" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-footer text-center">
                <a asp-action="Index" asp-controller="BasicDetail" class="btn btn-primary">Back To Index</a>
                <button type="submit" class="btn btn-primary">Proceed</button>
            </div>
        </form>
    </div>
</div>
@section Scripts{
    <script asp-src-include="~/lib/jquery-validate/jquery.validate.js"></script>
    <script asp-src-include="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script asp-src-include="~/js/CustomScript.js"></script>
    }