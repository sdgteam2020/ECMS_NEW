                                                <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlProIOFormation" required id="ddlProIOFormation">
                                                    <option value="0">Select Io Formation </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">IO Appt.</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlProIOAppointment" required id="ddlProIOAppointment">
                                                    <option value="0">Select IO Appointment </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">IO  Unit</label>
                                            <div class="col-sm-6">
                                                <i class="fas fa-asterisk mand"></i>
                                                <input class="form-control Alphanumeric" placeholder="Enter IO Army No" id="txtProIOformunit" required pattern="^[a-zA-Z0-9 ]*$" title="Only Alphabets and Numbers allowed" />
                                            </div>
                                            <div class="col-sm-2">
                                                <button type="button" class="btn btn-icon btn-round btn-warning" id="btnIOUnitAdd"><i class="fas fa-plus"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                   
                                    
                                </div>
                            </div>
                        </div>

                        <div class="card mt-2">
                            <div class="card-body">
                                <h5 class="card-title">GSO Details</h5>
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
