@{
    ViewData["Title"] = "Add Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container mt-4">
    <div class="card mb-3 ">
    <div class="card mb-3 ">
        <div class="card-header">
            <input class="form-control d-none" type="text" id="armyNo" value=@ViewBag.armyNo>
            <span id="SpnTxnId" class="d-none">0</span>
            @*<span id="PtoId" class="d-none">0</span>*@
            <div class="row">
                <div class="col-xl-12 text-left">
                    <h3>
                        Add Profile
                        <small class="text-muted"> Page</small>


                    </h3>
                </div>


            </div>
        </div>



    </div>
    <div class="card-body" style="min-height:150px;">

            <div class="row">



                <form action="" id="SaveFormProfile" method="post" name="SaveForm">
                    <div class="row">
                        <span class=" d-none" id="spnUnitMapUnitId">0</span>
                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title">Personal information</h5>
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Army No</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                <input class="form-control Alphanumeric" placeholder="Enter Army No" id="txtProArmy" required pattern="^[a-zA-Z0-9 ]*$" maxlength="8" title="Only Alphabets and Numbers allowed" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Rank</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlProRank" required id="ddlProRank">
                                                    <option value="0">Select Rank </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Name</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                <input class="form-control Alphanumeric" placeholder="Enter Name" id="txtName" required pattern="^[a-zA-Z0-9 ]*$" maxlength="8" title="Only Alphabets and Numbers allowed" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Formation</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlProFormation" required id="ddlProFormation">
                                                    <option value="0">Select Formation </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Appt.</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlProAppointment" required id="ddlProAppointment">
                                                    <option value="0">Select Appointment </option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card mt-2">
                            <div class="card-body">
                                <h5 class="card-title">Unit Details</h5>
                                <div class="row">
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Unit</label>
                                            <div class="col-sm-6">
                                                <i class="fas fa-asterisk mand"></i>
                                                <input class="form-control Alphanumeric" placeholder="Enter Unit" id="txtProUnit" required pattern="^[a-zA-Z0-9 ]*$" title="Only Alphabets and Numbers allowed" />
                                            </div>
                                            <div class="col-sm-2">
                                                <button type="button" class="btn btn-icon btn-round btn-warning" id="btnProUnitAdd"><i class="fas fa-plus"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Sus No</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                
                                                <input class="form-control Alphanumeric" placeholder="Enter SUS No" id="txtProSusno" required pattern="^[a-zA-Z0-9 ]*$" maxlength="8" title="Only Alphabets and Numbers allowed" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Comd</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label col-sm-2" id="lblProComd"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Corps</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label col-sm-2" id="lblProCorps"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Div</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label col-sm-2" id="lblProDiv"></label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-4">
                                        <div class="form-group row mb-2">
                                            <label class="col-form-label col-sm-4">Bde</label>
                                            <div class="col-sm-8">
                                                <label class="col-form-label col-sm-2" id="lblPrBde"></label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="card mt-2">
                            <div class="card-body">
                                <h5 class="card-title">IO Details</h5>
