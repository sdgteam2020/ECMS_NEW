                                            <label class="col-form-label col-sm-4">GSO Appt.</label>
                                            <div class="col-sm-8">
                                                <i class="fas fa-asterisk mand"></i>
                                                <select class=" js-example-basic-single select2 form-control" style="width: 100%;" tabindex="-1" aria-hidden="true" name="ddlProGSOAppointment" required id="ddlProGSOAppointment">
                                                    <option value="0">Select GSO Appointment </option>
                                                </select>
                                            </div>
