@{
    ViewData["Title"] = "Registered User";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .count-label {
        background-color: #044C92;
        text-align: center;
        padding: 0.5rem !important;
        margin-right: 0.25rem !important;
        border: hidden;
        font-weight: 700 !important;
        color: #fff !important;
        width: 120px;
        height: 60px;
    }
</style>
<header class="page-header page-header-dark bg-gradient-primary-to-secondary pb-10">
    <div class="containerMain">
        <div class="page-header-content pt-4">
            <div class="row align-items-center justify-content-between">
                <div class="col-auto">
                    <h1 class="page-header-title">
                        <div class="page-header-icon"> <i class="fa fa-bars"></i></div>
                        <a href="/Home/Index" class="mr-2">Home</a> > <a href="/Home/Dashboard" class="mr-2">Dashboard</a> > <a href="/Home/DashboardUserMgt" class="mr-2"> User Mngt</a> >Registered User

                    </h1>
                </div>
            </div>

        </div>
    </div>
</header>
<div class="containerMain mt-n10">
    <div class="card mb-3 ">
        <div class="card-header">

            <span id="UnitId" class="d-none">@ViewBag.UnitId</span>
            @*<span id="PtoId" class="d-none">0</span>*@
            <div class="row">
                <div class="col-xl-12 text-left">
                    <h3>
                        Registered User Details
                        <small class="text-muted"></small>
                    </h3>
                </div>
            </div>
        </div>
        <div class="card-body" style="min-height:150px;">
            <div class="row">
                <div class="col-xl-6">
                </div>
                <div class="col-xl-6">
                    <div class="d-flex d-md-flex justify-content-md-between align-items-md-center">
                        <h6 class="mb-3 mb-md-0">
                            @* Total Command <span id="lblTotal" class="ml-3 border border-light b px-2 py-1">0</span> *@
                        </h6>
                    </div>
                </div>
                <div class="table-responsive">
                    <table class="table border border-purple table-striped no-footer table-hover" role="grid" id="tbldata">
                        <thead>
                            <tr>
                                <th class="nowrap">S No</th>
                                <th class="nowrap">Domain ID</th>
                                <th class="nowrap">Army No</th>
                                <th class="nowrap">Rank</th>
                                <th class="nowrap">Name</th>
                                <th class="nowrap">Appt</th>
                            </tr>
                        </thead>
                        <tbody id="DetailBody">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<script asp-src-include="~/developerjs/RegisterUser.js"></script>